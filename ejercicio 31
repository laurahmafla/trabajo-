o=1000;
v1=100;
r1=5*o;
r4=150*o;
r3=200*o;


for i1= -0.001:0.0001:0.001
    i4=(-v1+r1*i1)/-r4;
end


r2=100*o;
r5=250*o;

fprintf('   i3          i5           v2\n')


for i3=-0.001:0.0001:0.001
for i2=-0.001:0.0001:0.001
    i5=(-r4*i4+r2*i2)/-r5;

v2=r5*i5-r3*i3;
if v2>0
disp([i3  i5  v2])    
end
end 
end
